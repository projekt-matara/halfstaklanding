// mixins

spacer(space)
  padding-top space
  padding-bottom space
compress(space)
  padding-left space
  padding-right space
center-object()
  display block
  margin-left auto
  margin-right auto
.center-object
  center-object()
tron(width)
  width width
  @extend .center-object
.nudge_down
  margin-top 2em
.headliner
  spacer(3em)
  tron(50%)
  h1, h2, h3, h4, h5, h6
    text-align center
    font-weight 300
  p
    text-align center
.light
  font-weight 300
.center
  text-align center
bg_image(name)
  background name center / cover

// core

body
	background-color: indigo
  
